{
  "name": "hello-world-argo",
  "version": "1.0.0",
  "description": "Cronjob application for ArgoCD demo",
  "main": "src/index.ts",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "start": "node dist/index.js",
    "build": "tsc",
    "build:bun": "bun build src/index.ts --outdir ./dist --target bun",
    "start:bun": "bun run src/index.ts",
    "dev:bun": "bun run --watch src/index.ts",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "axios": "^1.6.0"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "tsx": "^4.0.0",
    "typescript": "^5.0.0"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "packageManager": "pnpm@8.14.3+sha1.872b21adeaae4e71acd5c551ba848d927d6aa446"
}